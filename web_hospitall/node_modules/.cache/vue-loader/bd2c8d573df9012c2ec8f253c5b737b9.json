reset();\r\n            }\r\n        },\r\n        submitForm: async function () {\r\n            this.disabledButton = true;\r\n            this.loadingButton = true;\r\n            const result = await this.onSubmit();\r\n            if (result) {\r\n                this.closeModal();\r\n                this.loadingButton = false;\r\n                this.disabledButton = false;\r\n            }\r\n        }\r\n    }\r\n}\r\n</script>\r\n<style>\r\n.disabled {\r\n    opacity: 0.5;\r\n    cursor: not-allowed;\r\n}\r\n</style>"]}]}