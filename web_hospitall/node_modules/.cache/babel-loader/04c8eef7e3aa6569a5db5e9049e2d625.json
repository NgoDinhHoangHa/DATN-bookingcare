ive: false\r\n        }\r\n    },\r\n    computed: {\r\n        ...mapState(['user'])\r\n    },\r\n    methods: {\r\n        ...mapMutations(['setuser']),\r\n        logout: function () {\r\n            let auth = confirm('Bạn có muốn đăng xuất?')\r\n            if (auth) {\r\n                window.localStorage.removeItem('K-user')\r\n                this.setuser(null)\r\n                this.$router.push({ name: 'Login' });\r\n            }\r\n        }\r\n    }\r\n}\r\n</script>"]}]}