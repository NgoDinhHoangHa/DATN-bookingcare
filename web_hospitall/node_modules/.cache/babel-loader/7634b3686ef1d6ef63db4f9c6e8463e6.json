onents: {\r\n        ModalAdmin\r\n    },\r\n    computed: {\r\n        ...mapState(['modal'])\r\n    },\r\n    props: {\r\n        timeProp: String,\r\n        priceProp: String,\r\n        resultProp: String,\r\n        nameProp: String,\r\n        reasonProp: String,\r\n        list: Array,\r\n        title: String,\r\n        handleSubmit: Function,\r\n        setList: Function,\r\n        name: String,\r\n        table: String,\r\n        id: Number,\r\n        status: Number\r\n    },\r\n    methods: {\r\n        ...mapMutations(['setModal']),\r\n        setLoading: function (status) {\r\n            this.setModal({ ...this.modal, loading: status })\r\n        },\r\n        onChange: function (event) {\r\n            this.statusUpdate = event.target.value\r\n        },\r\n        onSubmit: async function () {\r\n            try {\r\n                const result = await Request.Post(`/${this.table}-status`, {\r\n                    id: this.id,\r\n                    status: this.statusUpdate\r\n                });\r\n                this.setList(result.data.data);\r\n                this.closeModal();\r\n            } catch (error) {\r\n                alert(error);\r\n            }\r\n        },\r\n        closeModal: function () {\r\n            this.setModal({ ...this.modal, loading: true, data: null })\r\n        },\r\n    },\r\n    data() {\r\n        return {\r\n            statusUpdate: `${this.status}`\r\n        }\r\n    },\r\n    mounted() {\r\n        this.setLoading(true);\r\n        this.setLoading(false);\r\n    }\r\n}\r\n</script>"]}]}