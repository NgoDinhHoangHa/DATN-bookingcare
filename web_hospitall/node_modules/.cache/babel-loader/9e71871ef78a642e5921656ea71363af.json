nB,IAAL,GAAYwB,KAAZ;MACJ;IACH,CAhCE;IAiCHC,UAAU;MAAA,0EAAE,kBAAgBvB,EAAhB;QAAA;QAAA;UAAA;YAAA;cAAA;gBAAA;gBAEJ,KAAKE,aAAL,GAAqBF,EAArB;gBAFI;gBAAA,OAGiBR,OAAO,CAACgC,MAAR,oBAA2BxB,EAA3B,EAHjB;;cAAA;gBAGEc,MAHF;;gBAIJ,IAAIA,MAAM,CAACjB,IAAP,CAAY4B,MAAhB,EAAwB;kBACpB,KAAK3B,IAAL,GAAY,mBAAI,KAAKA,IAAT,EAAe4B,MAAf,CAAsB,YAAC;oBAAA,OAAKN,EAAE,CAACC,OAAH,KAAerB,EAApB;kBAAA,CAAvB,CAAZ;gBACJ;;gBACA,KAAKE,aAAL,GAAqB,CAAC,CAAtB;gBAPI;gBAAA;;cAAA;gBAAA;gBAAA;gBASJa,KAAK,cAAL;;cATI;cAAA;gBAAA;YAAA;UAAA;QAAA;MAAA,CAAF;;MAAA;QAAA;MAAA;;MAAA;IAAA,GAjCP;IA6CHY,QAAQ,EAAE,kBAAU3B,EAAV,EAAc;MACpB,KAAKA,EAAL,GAAUA,EAAV;MACA,KAAK4B,QAAL,iCAAmB,KAAKC,KAAxB;QAA+BhC,IAAI,EAAE;MAArC;IACJ;EAhDG,EAZI;EA8DXiC,QAAQ,oBACDnC,QAAQ,CAAC,CAAC,OAAD,CAAD,CADP,CA9DG;EAiEXoC,UAAU,EAAE;IACRzC,IAAI,EAAJA,IADQ;IAERC,KAAK,EAALA,KAFQ;IAGRE,WAAU,EAAVA;EAHQ,CAjED;EAsEXuC,OAtEW,qBAsED;IAAA;;IACN,wDAAC;MAAA;MAAA;QAAA;UAAA;YAAA;cAAA;cAAA;cAAA,OAE4BxC,OAAO,CAACyC,GAAR,CAAY,eAAZ,CAF5B;;YAAA;cAEanB,MAFb;cAGO,MAAI,CAAChB,IAAL,GAAYgB,MAAM,CAACjB,IAAP,CAAYA,IAAxB;cACA,MAAI,CAACE,OAAL,GAAe,KAAf;cAJP;cAAA;;YAAA;cAAA;cAAA;cAMOgB,KAAK,cAAL;;YANP;YAAA;cAAA;UAAA;QAAA;MAAA;IAAA,CAAD;EASJ;AAhFW,CAAf","names":["Base","Table","Request","ModalDoctor","mapMutations","mapState","URL_IMAGE","data","list","loading","id","urlImage","loadingColumn","timeOut","counter","methods","reset","onInput","event","clearTimeout","setTimeout","Post","value","target","result","alert","setList","item","index","findIndex","dt","idadmin","clone","deleteItem","Delete","status","filter","editItem","setModal","modal","computed","components","mounted","Get"],"sourceRoot":"","sources":["C:\\Users\\OS\\OneDrive - University of Technology and Education\\Desktop\\DATN-bookingcare\\web_hospitall\\src\\components\\manage\\pages\\Doctors.vue"],"sourcesContent":["<template>\r\n    <Base :onInput=\"onInput\">\r\n    <Table :loading=\"loading\" :title=\"'Danh sách bác sĩ'\" :list=\"list\" :length=\"'8'\"\r\n        :heading=\"['Ảnh đại diện', 'Số điện thoại ', 'Chuyên khoa', 'Họ tên', 'Tên đăng nhập']\" >\r\n        <tr v-for=\"(item, key) in list\" :key=\"item.id\">\r\n            <td>{{ key + 1 }}</td>\r\n            <td><img :src=\"urlImage + item.avatar\"\r\n                    style=\"border-radius:100rem;width: 6rem;height:6rem;object-fit: cover;\" alt=\"\" srcset=\"\">\r\n            </td>\r\n            <td>{{ item.sdt }}</td>\r\n            <td>{{ item.namespecical }}</td>\r\n            <td>{{ item.name }}</td>\r\n            <td>{{ item.username }}</td>\r\n            <td>\r\n                <span @click=\"editItem(item.idadmin)\" class=\"bx bx-pencil\"></span>\r\n            </td>\r\n            <td>\r\n                <span @click=\"deleteItem(item.idadmin)\"\r\n                    :class=\"`bx bx-${loadingColumn === item.id ? 'loader loading' : 'trash'}`\"></span>\r\n            </td>\r\n        </tr>\r\n    </Table>\r\n    <ModalDoctor v-if=\"modal.data\" :reset=\"reset\" :id=\"id\" :setList=\"setList\" :list=\"list\"></ModalDoctor>\r\n    </Base>\r\n\r\n</template>\r\n<script>\r\nimport Base from \"./Base.vue\";\r\nimport Table from \"../Table.vue\";\r\nimport Request from \"../../../Request\";\r\nimport ModalDoctor from \"../modal/ModalDoctor.vue\";\r\nimport { mapMutations, mapState } from \"vuex\";\r\nimport { URL_IMAGE } from \"../../../Config\";\r\nexport default {\r\n    data() {\r\n        return {\r\n            list: [],\r\n            loading: true,\r\n            id: null,\r\n            urlImage: URL_IMAGE,\r\n            loadingColumn: -1,\r\n            timeOut: null,\r\n            counter: 0\r\n        }\r\n    },\r\n    methods: {\r\n        ...mapMutations(['setModal']),\r\n        reset: function () {\r\n            this.id = null;\r\n            this.data = null;\r\n        },\r\n        onInput: function (event) {\r\n            this.loading = true;\r\n            this.counter += 1;\r\n            clearTimeout(this.timeOut);\r\n            this.timeOut = setTimeout(async () => {\r\n                try {\r\n                    const result = await Request.Post(`/doctor-search`, {\r\n                        value: event.target.value\r\n                    });\r\n                    this.list = result.data.data;\r\n                    this.loading = false;\r\n                } catch (error) {\r\n                    alert(error);\r\n                }\r\n            }, 300);\r\n        },\r\n        setList: function (item) {\r\n            const index = [...this.list].findIndex(dt => dt.idadmin === item.idadmin);\r\n            if (index === -1) {\r\n                this.list = [...this.list, item];\r\n            }\r\n            else {\r\n                let clone = [...this.list];\r\n                clone[index] = item;\r\n                this.list = clone;\r\n            }\r\n        },\r\n        deleteItem: async function (id) {\r\n            try {\r\n                this.loadingColumn = id;\r\n                const result = await Request.Delete(`/doctors/${id}`);\r\n                if (result.data.status) {\r\n                    this.list = [...this.list].filter(dt => dt.idadmin !== id);\r\n                }\r\n                this.loadingColumn = -1;\r\n            } catch (error) {\r\n                alert(error);\r\n            }\r\n        },\r\n        editItem: function (id) {\r\n            this.id = id;\r\n            this.setModal({ ...this.modal, data: true });\r\n        }\r\n    },\r\n    computed: {\r\n        ...mapState(['modal'])\r\n    },\r\n    components: {\r\n        Base,\r\n        Table,\r\n        ModalDoctor\r\n    },\r\n    mounted() {\r\n        (async () => {\r\n            try {\r\n                const result = await Request.Get('/doctor-admin');\r\n                this.list = result.data.data;\r\n                this.loading = false;\r\n            } catch (error) {\r\n                alert(error)\r\n            }\r\n        })();\r\n    }\r\n}\r\n</script>\r\n<style scoped>\r\n    table{\r\n        border-collapse: collapse;\r\n        width: 100%;\r\n        color: #333;\r\n        font-family: Arial, sans-serif;\r\n        font-size: 14px;\r\n        text-align: left;\r\n        border-radius: 10px;\r\n        overflow: hidden;\r\n        box-shadow: 0 0 20px rgba(0, 0, 0, 0.1);\r\n        margin: auto;\r\n        margin-top: 50px;\r\n        margin-bottom: 50px;\r\n    }\r\n    table th {\r\n        background-color: #ff9800;\r\n        color: #fff;\r\n        font-weight: bold;\r\n        padding: 10px;\r\n        text-transform: uppercase;\r\n        letter-spacing: 1px;\r\n        border-top: 1px solid #fff;\r\n        border-bottom: 1px solid #ccc;\r\n    }\r\n    table tr:nth-child(even) td {\r\n        background-color: #f2f2f2;\r\n    } \r\n    table tr:hover td {\r\n    background-color: #E8E8E8;\r\n    }\r\n    table td {\r\n        background-color: #fff;\r\n        padding: 10px;\r\n        border-bottom: 1px solid #ccc;\r\n        font-weight: 500;\r\n    }\r\n</style>"]}]}